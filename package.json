{
  "name": "WeatherApp",
  "version": "1.0.0",
  "description": "Weather App using Angular 2",
  "main": "build/src/app/app.module.js",
  "scripts": {
    "cleanup": "rimraf src/app/**/*.ngfactory.ts src/app/**/*.ngstyle.ts src/app/**/*.ngsummary.json build dist",
    "explorer": "source-map-explorer ./dist/main.bundle.js ./dist/main.bundle.js.map",
    "ngc": "ngc",
    "build": "npm run cleanup && npm run ngc && webpack --config webpack.aot.config.js -p",
    "aot": "npm run cleanup && npm run ngc && concurrently -r \"webpack-dev-server --config webpack.aot.config.js\" \"node ./bin/ngc-watch.js\"",
    "jit": "npm run cleanup && webpack-dev-server --config webpack.jit.config.js",
    "lint": "tslint \"src/app/**/*.ts\""
  },
  "keywords": [
    "angular2",
    "aot",
    "typescript",
    "weatherapp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamanovich/weatherapp/"
  },
  "author": {
    "name": "Siarhei Hamanovich",
    "email": "hamanovich@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hamanovich/weatherapp/issues"
  },
  "dependencies": {
    "@angular/common": "2.4.4",
    "@angular/compiler": "2.4.4",
    "@angular/core": "2.4.4",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/platform-browser": "2.4.4",
    "@angular/platform-browser-dynamic": "2.4.4",
    "@angular/router": "3.4.4",
    "core-js": "2.4.1",
    "rxjs": "5.0.3",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.4",
    "@angular/platform-server": "2.4.4",
    "@types/es6-shim": "^0.31.32",
    "@types/googlemaps": "^3.26.0",
    "@types/node": "7.0.0",
    "@types/zone.js": "0.0.27",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "chokidar": "1.6.1",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "gh-pages": "0.12.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "0.9.0",
    "node-sass": "^4.3.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.2.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.1",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "tslint": "^4.3.1",
    "typescript": "2.0.10",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  }
}
